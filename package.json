{
  "name": "ruler-and-compass",
  "version": "0.10.0-dev",
  "description": "Ruler and Compass",
  "main": "src/main.js",
  "scripts": {
    "test": "jest",
    "test:clean": "rm -rf ./coverage",
    "test:fresh": "npm run test:clean && npm test",
    "test:coverage": "jest --coverage && open ./coverage/lcov-report/index.html",
    "test:cc": "rm -rf ./coverage; npm run test:coverage",
    "start": "npm test && grunt serve",
    "dist": "grunt dist",
    "docs": "grunt --debug jsdoc && open ./docs/index.html",
    "docs:clean": "rm -rf ./coverage",
    "docs:fresh": "npm run docs:clean && npm run docs"
  },
  "author": "Maic López Sáenz",
  "license": "MIT",
  "devDependencies": {
    "grunt": "^1.3.0",
    "grunt-browserify": "^5.3.0",
    "grunt-contrib-connect": "^3.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-exec": "^3.0.0",
    "grunt-jsdoc": "^2.4.1",
    "jest": "^26.6.3"
  },
  "dependencies": {
    "npm": "^7.6.0"
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.tests.js"
    ]
  }
}
